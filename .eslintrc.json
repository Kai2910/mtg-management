{
    "extends": "airbnb",
    "rules": {
        "import/extensions": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "react/prop-types": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "no-param-reassign": 0,
        "no-cond-assign": 0,
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "max-len": [2,  { "code": 140 } ]
    },
    "env": {
        "browser": true,
        "node": true
    }
}
